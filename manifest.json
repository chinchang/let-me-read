{
"name": "Let Me Read",
"version": "1.0",
"manifest_version": 2,
"description": "Provides a distraction free reading.",
"browser_action": {
 "default_icon": "icon.png"
},
"background": {
 "scripts": ["eventPage.js"],
 "persistent": false
},
"permissions": [
 "tabs", "http://*/*", "https://*/*"
]
}
